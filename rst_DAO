Dim dbs As DAO.Database
Dim rst As DAO.Recordset
Dim mySql As String

    'Definimos las variables

 mySql = ""
 Set dbs = CurrentDb
 Set rst = dbs.OpenRecordset(mySql, dbOpenSnapshot)

    'Limpiamos memoria
 If Not rst Is Nothing Then
     rst.Close
     Set rst = Nothing
    End If

 If Not dbs Is Nothing Then
     dbs.Close
     Set dbs = Nothing
 End If
